created:
    - database/factories/CategoryFactory.php
    - database/factories/EventFactory.php
    - database/factories/TicketFactory.php
    - database/factories/OrderFactory.php
    - database/factories/OrderDetailFactory.php
    - database/migrations/2026_01_17_092029_create_categories_table.php
    - database/migrations/2026_01_17_092030_create_events_table.php
    - database/migrations/2026_01_17_092031_create_tickets_table.php
    - database/migrations/2026_01_17_092032_create_orders_table.php
    - database/migrations/2026_01_17_092033_create_order_details_table.php
    - app/Models/Category.php
    - app/Models/Event.php
    - app/Models/Ticket.php
    - app/Models/Order.php
    - app/Models/OrderDetail.php
models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable', two_factor_secret: 'text nullable', two_factor_recovery_codes: 'text nullable', two_factor_confirmed_at: 'datetime nullable' }
    Category: { name: string, relationships: { hasMany: Event } }
    Event: { user_id: foreignId, category_id: foreignId, title: string, description: 'text nullable', location: string, datetime: datetime, image_path: 'string nullable', relationships: { belongsTo: 'User, Category', hasMany: 'Ticket, Order' } }
    Ticket: { event_id: foreignId, type: string, price: 'decimal:12,2', stock: integer, relationships: { belongsTo: Event, hasMany: OrderDetail } }
    Order: { user_id: foreignId, event_id: foreignId, order_date: datetime, total_price: 'decimal:12,2', relationships: { belongsTo: 'User, Event', hasMany: OrderDetail } }
    OrderDetail: { order_id: foreignId, ticket_id: foreignId, quantity: integer, sub_total: 'decimal:12,2', relationships: { belongsTo: 'Order, Ticket' } }
